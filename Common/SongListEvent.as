package {	import flash.events.Event;	public class SongListEvent extends Event	{		// Interface methods:		public static const SONG_LIST:String = "song_list";		public static const SONG_CATEGORY:String = "song_category";		public static const SONG_CONFIRM:String = "song_confirm";		public var data;		public function SongListEvent(type:String, data: *, bubbles:Boolean=false, cancelable:Boolean=false)		{			super(type, bubbles, cancelable);			this.data = data;		}		override public function clone():Event		{			return new SongListEvent (type, data, bubbles, cancelable);		}	}}